<p id="notice"><%= notice %></p>

<h1>Inventories</h1>

<table>
  <thead>
    <tr>
      <th>Item dec</th>
      <th>Serial num</th>
      <th>Status</th>
      <th>Status date</th>
      <th>Agent rec</th>
      <th>Incident rep</th>
      <th>Nsn in inventory</th>
      <th>Notes</th>
      <th>Expendable</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inventories.each do |inventory| %>
      <tr>
        <td><%= inventory.item_dec %></td>
        <td><%= inventory.serial_num %></td>
        <td><%= inventory.status %></td>
        <td><%= inventory.status_date %></td>
        <td><%= inventory.agent_rec %></td>
        <td><%= inventory.incident_rep %></td>
        <td><%= inventory.nsn_in_inventory %></td>
        <td><%= inventory.notes %></td>
        <td><%= inventory.expendable %></td>
        <td><%= link_to 'Show', inventory %></td>
        <td><%= link_to 'Edit', edit_inventory_path(inventory) %></td>
        <td><%= link_to 'Destroy', inventory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inventory', new_inventory_path %>
