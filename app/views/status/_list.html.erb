<table class="data">
	<thead>
		<tr>
			<th>StatusRecNumber</th>
			<th>Status</th>
     	<th>StatusDescription</th>    
		</tr>
	</thead>
	<tbody>
		<% reset_cycle %>
		<% objs.each { |o| %>
			<tr class="<%= cycle 'odd', 'even' %>">
				<td class="nobr"><%= o.id %></td>
				<td><%= link_to o.status, :action => :view, :id => o.id %></td>
				<td class="nobr"><%= o.status_description %></td>
			</tr>
		<% } %>
	</tbody>
</table>