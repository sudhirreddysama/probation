<% if session["report"] && "agent_records".eql?(session["report"][:type]) %>
	<% @agents = Inventory.where(agent_rec: session["report"][:data]) %>
	<%= render "agent_records" %>
<% elsif session["report"] && "Inv_assign_report".eql?(session["report"][:type]) %>
	<% @items = Inventory.where(item_dec: session["report"][:data]) %>
	<%= render "inv_assign_report" %>
<%end%>
<% session["report"] = nil %>