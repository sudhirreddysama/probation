<table class="data">
	<thead>
		<tr>
		      <th>Description</th>
		      <th>Assigned to:</th>
		      <th>Status</th>
		</tr>
	</thead>
	<tbody>
		<% reset_cycle %>
		<% objs.each { |o| %>
			<tr class="<%= cycle 'odd', 'even' %>">
				<td><%= link_to o.item_dec, :action => :view, :id => o.id  %></td>
				<%if o.agent_rec %>
					<td class="nobr"><%= Agent.find(o.agent_rec).full_name %></td>
				<%else%>
				<td></td>
				<% end %>
				<td class="nobr"><%= o.status %></td>
			</tr>
		<% } %>
	</tbody>
</table>